<p id="notice"><%= notice %></p>

<h1>Vehicule Brands</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicule_brands.each do |vehicule_brand| %>
      <tr>
        <td><%= vehicule_brand.uid %></td>
        <td><%= vehicule_brand.name %></td>
        <td><%= vehicule_brand.description %></td>
        <td><%= vehicule_brand.status %></td>
        <td><%= vehicule_brand.user %></td>
        <td><%= link_to 'Show', vehicule_brand %></td>
        <td><%= link_to 'Edit', edit_vehicule_brand_path(vehicule_brand) %></td>
        <td><%= link_to 'Destroy', vehicule_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicule Brand', new_vehicule_brand_path %>
